<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.persistence.dao.MtimeYearDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.persistence.model.MtimeYearDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>

    <resultMap id="YearBoResultMap" type="com.stylefeng.guns.rest.persistence.model.bo.filmBo.YearBo">
        <id column="UUID" property="yearId" />
        <result column="show_name" property="yearName" />
    </resultMap>

    <select id="listAllYear" resultMap="YearBoResultMap">
        select UUID, show_name from mtime_year_dict_t
    </select>

    <select id="selectYearById" resultMap="YearBoResultMap">
        select UUID, show_name from mtime_year_dict_t where UUID = #{id}
    </select>


</mapper>
